application: gae-atompub-server
version: %BUILD%
runtime: python27
api_version: 1
threadsafe: true

inbound_services:
- warmup

libraries:
- name: pycrypto
  version: "2.6"
- name: django
  version: "1.2"

handlers:
- url: /static
  static_dir: static
  expiration: "365d"
- url: /.*
  script: atompub.application

